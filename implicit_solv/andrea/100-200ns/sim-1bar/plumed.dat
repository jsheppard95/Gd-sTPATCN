MOLINFO MOLTYPE=protein STRUCTURE=2v1a_min.pdb

# radius of gyration
rgyr: GYRATION TYPE=RADIUS ATOMS=1-2344

# antiparallel beta
ab: ANTIBETARMSD RESIDUES=all LESS_THAN={RATIONAL R_0=0.08 NN=8 MM=12} STRANDS_CUTOFF=1

# parallel beta
pb: PARABETARMSD RESIDUES=all LESS_THAN={RATIONAL R_0=0.08 NN=8 MM=12} STRANDS_CUTOFF=1

# alpha helix
alfa: ALPHARMSD RESIDUES=all LESS_THAN={RATIONAL R_0=0.08 NN=8 MM=12}

# sasar
#sasar: SASA_HASEL ATOMS=1-2344 TYPE=TRANSFER NL_STRIDE=10 DELTAGFILE=DeltaG.dat

#bias: BIASVALUE ARG=sasar

PRINT ... 
 ARG=rgyr,alfa.lessthan,ab.lessthan,pb.lessthan #,sasar,bias.*
 STRIDE=1000
 FILE=COLVAR
... PRINT

